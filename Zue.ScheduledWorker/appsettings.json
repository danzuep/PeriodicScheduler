{
  "CronScheduling": {
    "Name": "Periodic Scheduler",
    // Container timezone is UTC, so 5am NZ is 16
    "Expression": "0 16 ? * *",
    "AutoStart": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}
